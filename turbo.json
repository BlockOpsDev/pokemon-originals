{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": [
    "$WORKER_KV_KEY",
    "$WORKER_ACCOUNT_ID",
    "$WORKER_KV_NAMESPACE_ID"
  ],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "clean": {
      "cache": false
    }
  }
}
